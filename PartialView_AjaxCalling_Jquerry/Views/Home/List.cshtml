@model  IEnumerable<PartialView_AjaxCalling_Jquerry.Models.Student>
    @{
        ViewBag.Title = "List";
    }
    <h2>List</h2>
    <p>
        @Html.Label("Top 5",htmlAttributes:new  { @class ="showList",@style="cursor:pointer;",data_rows =5})
    </p>
<p>
    @Html.Label("Top 10", htmlAttributes: new { @class = "showList", @style = "cursor:pointer;", data_rows = 10 })
</p>

<div id="student_list_container">
</div>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script>
    $('.showList').click(function (e) {
     
        var row_num = this.getAttribute("data-rows");
        $.ajax({
            type: "GET",
            url: "/Employee/EmployeePartialList",
            data: {id: row_num },
            success: function (data) {
                $("#student_list_container").html(data);
            },
            error: function () {
                alert("error");
            }
        });
    });

</script>
